                <mxCell id="229" value="int a1 = &lt;font style=&quot;color: rgb(255, 0, 0);&quot;&gt;32&lt;/font&gt;;" style="rounded=0;whiteSpace=wrap;html=1;fontSize=12;fillColor=#CCFFCC;" parent="1" vertex="1">
